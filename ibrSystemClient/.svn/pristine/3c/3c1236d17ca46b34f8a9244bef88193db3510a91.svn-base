.tkn_manu > ul
{
    background: rgb(57,108,153); /* Old browsers */
    background: -moz-linear-gradient(left,  rgba(57,108,153,1) 0%, rgba(47,94,136,1) 51%, rgba(39,83,122,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(57,108,153,1)), color-stop(51%,rgba(47,94,136,1)), color-stop(100%,rgba(39,83,122,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left,  rgba(57,108,153,1) 0%,rgba(47,94,136,1) 51%,rgba(39,83,122,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left,  rgba(57,108,153,1) 0%,rgba(47,94,136,1) 51%,rgba(39,83,122,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left,  rgba(57,108,153,1) 0%,rgba(47,94,136,1) 51%,rgba(39,83,122,1) 100%); /* IE10+ */
    background: linear-gradient(to right,  rgba(57,108,153,1) 0%,rgba(47,94,136,1) 51%,rgba(39,83,122,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#396c99', endColorstr='#27537a',GradientType=1 ); /* IE6-9 */

    margin: 0;
    margin-right:0;
    width: 100%;
    padding: 0;
    display:inline-block;
    height:45px;

    -webkit-border-radius: 10px;-moz-border-radius: 10px;border-radius: 10px;
    list-style-type: none;
}

.tkn_manu > ul > li
{
    color: #ffffff;
    float: right;
    font-weight: bold;
    line-height: 39px;
    margin-top: 3px;
    text-align: center;
    width: 18%;
    margin-right:1.5%;
    -webkit-border-radius: 9px;-moz-border-radius: 9px;border-radius: 9px;
    position:relative;
}

.tkn_manu > ul > li .tkn_sub_menu
{
    background-color: #ffffff;
    border: 2px solid #173f64;
    list-style-type: none;
    margin: 0;
    padding: 0;
    position: absolute;
    right: -25px;
    top: 41px;
    width: 365px;
    -webkit-border-radius: 9px;-moz-border-radius: 9px;border-radius: 9px;
    display:none;
}

.tkn_manu > ul > li .tkn_sub_menu li
{
    font-size: 13px;
    line-height: 31px;
    text-align: right;
    text-indent: 10px;
    padding: 0 6px;
}

.tkn_manu > ul > li .tkn_sub_menu li > a
{
    border-top: 1px solid #173f64;
    display:block;
    color: #173f64;
    text-decoration:none;
}

.tkn_manu > ul > li .tkn_sub_menu li:first-child > a
{
    border-top: none;
}

.tkn_manu > ul > li .tkn_sub_menu li:first-child
{
    -webkit-border-top-left-radius: 9px;-webkit-border-top-right-radius: 9px;-moz-border-radius-topleft: 9px;
    -moz-border-radius-topright: 9px;border-top-left-radius: 9px;border-top-right-radius: 9px;
}

.tkn_manu > ul > li .tkn_sub_menu li:last-child
{
    -webkit-border-bottom-right-radius: 9px;-webkit-border-bottom-left-radius: 9px;-moz-border-radius-bottomright: 9px;
    -moz-border-radius-bottomleft: 9px;border-bottom-right-radius: 9px;border-bottom-left-radius: 9px;
}

.tkn_manu > ul > li .tkn_sub_menu li:hover
{
    color:#6a1042;
    background-color:#e1e1e1;
}

.tkn_manu > ul > li a
{
    display:block;
    color:#ffffff;
    text-decoration:none;
}

.tkn_manu > ul > li:hover,
.tkn_manu > ul > li.tbk_menu_show
{

    cursor:pointer;
    background: rgb(169,51,117); /* Old browsers */
    background: -moz-linear-gradient(top,  rgba(169,51,117,1) 0%, rgba(130,29,85,1) 50%, rgba(106,16,66,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(169,51,117,1)), color-stop(50%,rgba(130,29,85,1)), color-stop(100%,rgba(106,16,66,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(169,51,117,1) 0%,rgba(130,29,85,1) 50%,rgba(106,16,66,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(169,51,117,1) 0%,rgba(130,29,85,1) 50%,rgba(106,16,66,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(169,51,117,1) 0%,rgba(130,29,85,1) 50%,rgba(106,16,66,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(169,51,117,1) 0%,rgba(130,29,85,1) 50%,rgba(106,16,66,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#a93375', endColorstr='#6a1042',GradientType=0 ); /* IE6-9 */
}

.tkn_manu > ul > li:hover > a,
.tkn_manu > ul > li.tbk_menu_show > a
{

    color:#ffffff !important;
}

.tkn_manu > ul > li.selected
{
    background-color:#a93375;
}

.tkn_manu > ul > li.selected > a
{

}

