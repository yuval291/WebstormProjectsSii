.tkn_form
{
}
.tkn_form table {
    width: 90%;
}
.tkn_form table tr {
    background-color: #eaf4fd;
}
.tkn_form table tr td {
    line-height: 38px;
    text-align: right;
}

.tkn_form table tr td[data-col="icon"]
{
    text-align: center;
}

.tkn_form table tr td[data-col="title"]
{
    color: #163f62;
    text-indent: 12px;
    width: 22%;
}

.tkn_form table tr td[data-col="field"]
{

}

.tkn_form table tr td[data-col="valid"]
{
    text-align: center;
    width: 20%;
}

.tkn_form table tr td input
{
    border: 1px solid #cbced3;
    height: 29px;
    width: 98%;
    margin-right: 3%;
    text-indent: 4px;
    width: 94%;
    -webkit-border-radius: 9px;-moz-border-radius: 9px;border-radius: 9px;
}
.tkn_form table tr td select
{
    border: 1px solid #cbced3;
    margin-right: 3%;
    padding: 1.4%;
    width: 95%;
    -webkit-border-radius: 9px;-moz-border-radius: 9px;border-radius: 9px;
}
.tkn_form table tr.alter {
    background-color: #e8e8e8;
}
.tkn_form table tr.alter td {
}
.tkn_form table tr td.tkn_validation_alert div
{
    color: #c22221;
    font-size: 12px;
    font-weight: bold;
    visibility:hidden;
}

.tkn_form table tr td.tkn_validation_alert.show div
{
    visibility:visible;
}

