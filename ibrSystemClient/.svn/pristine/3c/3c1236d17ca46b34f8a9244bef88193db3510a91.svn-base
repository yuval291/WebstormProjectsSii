body
{
    background: rgb(228,238,247); /* Old browsers */
    background: -moz-linear-gradient(top,  rgba(228,238,247,1) 0%, rgba(239,242,247,1) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(228,238,247,1)), color-stop(100%,rgba(239,242,247,1))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(228,238,247,1) 0%,rgba(239,242,247,1) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(228,238,247,1) 0%,rgba(239,242,247,1) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(228,238,247,1) 0%,rgba(239,242,247,1) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(228,238,247,1) 0%,rgba(239,242,247,1) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e4eef7', endColorstr='#eff2f7',GradientType=0 ); /* IE6-9 */

    font-family:Arial;
    margin:0;
    padding:0;
    overflow-y:scroll;
}

body,html
{
    width:100%;
    min-height:100%;
    font-size: 17px !important;
}

pre {
    white-space: pre-wrap !important; /* CSS 3 */
    white-space: -moz-pre-wrap !important; /* Mozilla, since 1999 */
    white-space: -pre-wrap !important; /* Opera 4-6 */
    white-space: -o-pre-wrap !important; /* Opera 7 */
    /* Internet Explorer 5.5+ */
    font-family: arial;
    border: 0 !important;
    background-color: white;
    word-break: normal;
}
